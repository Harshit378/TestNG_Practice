<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Lets Kode It Parallel">
	<parameter name="browser" value="chrome"></parameter>
	<test name="Lets Kode It - Chrome" preserve-order="true"
		enabled="false">
		<classes>
			<class name="listeners.AutomationDriver"></class>
			<class name="automation.Test_LetsCodeIt"></class>
		</classes>
	</test>
	<test name="POP-UP : Chrome" preserve-order="true" enabled="true">
		<classes>
			<class name="listeners.AutomationDriver"></class>
			<class name="automation.Test_DropDown"></class>
		</classes>
	</test>
</suite>